<effect looping="false" layer="20">
	
	<param name="radius" default="0" />
	<param name="angle" default="0" />
	<variation>
		<sprite origin="11 8" looping="false" texture="./chaos_blade_hit.png" material="system/default.mats:bright-noxray"
		angle="rand(u+i)*pi*2"
		pos-x="cos(angle)*2"
		pos-y="-4+sin(angle)*2"
		color-a="sin(t*pi)*0.5"
		>
			<frame time="30">0 0 32 23</frame>
			<frame time="30">32 0 32 23</frame>
			<frame time="90">64 0 32 23</frame>
			<frame time="60">96 0 32 23</frame>
		</sprite>
		<sprite origin="11 8" looping="false" texture="./chaos_blade_hit.png" material="system/default.mats:bright-noxray"
		angle="rand(u+i)*pi*2"
		pos-x="cos(angle)*2"
		pos-y="-4+sin(angle)*2"
		color-a="sin(t*pi)*0.5"
		>
			<frame time="90">0 0 0 0</frame>
			<frame time="30">0 23 32 23</frame>
			<frame time="30">32 23 32 23</frame>
			<frame time="90">64 23 32 23</frame>
			<frame time="30">96 23 32 23</frame>
		</sprite>
		%//white glow
		<sprite origin="11 8" looping="false" texture="./chaos_blade_hit.png" material="system/default.mats:bright-glow"
		angle="rand(u+i)*pi*2"
		pos-x="cos(angle)*2"
		pos-y="-4+sin(angle)*2"
		>
			<frame time="60">0 47 32 23</frame>
			<frame time="60">32 47 32 23</frame>
			<frame time="30">64 47 32 23</frame>
			<frame time="30">96 47 32 23</frame>
		</sprite>
		<sprite origin="11 8" looping="false" texture="./chaos_blade_hit.png" material="system/default.mats:bright-glow"
		angle="rand(u+i)*pi*2"
		pos-x="cos(angle)*2"
		pos-y="-4+sin(angle)*2"
		>
			<frame time="90">0 0 0 0</frame>
			<frame time="60">0 70 32 23</frame>
			<frame time="60">32 70 32 23</frame>
			<frame time="30">64 70 32 23</frame>
			<frame time="30">96 70 32 23</frame>
		</sprite>
	</variation>
	
	<variation>
		<sprite origin="11 8" looping="false" texture="./chaos_blade_hit.png" material="system/default.mats:bright-noxray"
		angle="rand(u+i)*pi*2"
		pos-x="cos(angle)*2"
		pos-y="-4+sin(angle)*2"
		color-a="sin(t*pi)*0.5"
		>
			<frame time="90">0 0 0 0</frame>
			<frame time="60">0 0 32 23</frame>
			<frame time="30">32 0 32 23</frame>
			<frame time="90">64 0 32 23</frame>
			<frame time="60">96 0 32 23</frame>
		</sprite>
		<sprite origin="11 8" looping="false" texture="./chaos_blade_hit.png" material="system/default.mats:bright-noxray"
		angle="rand(u+i)*pi*2"
		pos-x="cos(angle)*2"
		pos-y="-4+sin(angle)*2"
		color-a="sin(t*pi)*0.5"
		>
			<frame time="60">0 23 32 23</frame>
			<frame time="60">32 23 32 23</frame>
			<frame time="90">64 23 32 23</frame>
			<frame time="60">96 23 32 23</frame>
		</sprite>
		%//white glow
		<sprite origin="11 8" looping="false" texture="./chaos_blade_hit.png" material="system/default.mats:bright-glow"
		angle="rand(u+i)*pi*2"
		pos-x="cos(angle)*2"
		pos-y="-4+sin(angle)*2"
		>
			<frame time="90">0 0 0 0</frame>
			<frame time="60">0 47 32 23</frame>
			<frame time="60">32 47 32 23</frame>
			<frame time="30">64 47 32 23</frame>
			<frame time="30">96 47 32 23</frame>
		</sprite>
		<sprite origin="11 8" looping="false" texture="./chaos_blade_hit.png" material="system/default.mats:bright-glow"
		angle="rand(u+i)*pi*2"
		pos-x="cos(angle)*2"
		pos-y="-4+sin(angle)*2"
		>
			<frame time="30">0 70 32 23</frame>
			<frame time="60">32 70 32 23</frame>
			<frame time="60">64 70 32 23</frame>
			<frame time="30">96 70 32 23</frame>
